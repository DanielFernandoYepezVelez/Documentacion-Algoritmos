//ESCRIBA UN ALGORITMO QUE LEA UN NUMERO Y QUE DIGA SI FUE 0 O 1.

Proceso Dos
	
	Definir numerito como entero;
	
	Escribir "ingresar un numero";
	leer numerito;
	
	si (numerito=0) entonces
		Escribir "el numero ingresado es CERO";
	SiNo
		si (numerito=1) entonces
			Escribir "el numero ingresado es UNO";
		FinSi
	FinSi

FinProceso
